{
  "name": "Mock Cicada default theme",
  "author": "cicada AG",
  "description": {
    "en-GB": "Default theme of Cicada 6",
    "zh-CN": "Standard Theme von Cicada 6"
  },
  "previewMedia": "app/storefront/dist/assets/defaultThemePreview.jpg",
  "style": [
    {
      "app/storefront/src/scss/base.scss": {
        "resolve": {
          "vendor": "app/storefront/vendor"
        }
      },
      "app/storefront/src/scss/skin/cicada/_base.scss": {
        "resolve": {
          "vendor": "app/storefront/vendor"
        }
      }
    },
    "@Plugins"
  ],
  "script": [
    "app/storefront/dist/js/vendor-node.js",
    "app/storefront/dist/js/vendor-shared.js",
    "app/storefront/dist/js/runtime.js",
    "app/storefront/dist/storefront/js/storefront.js",
    "@Plugins"
  ],
  "asset": [
    "app/storefront/dist/assets"
  ],
  "config": {
    "blocks": {
      "themeColors": {
        "label": {
          "en-GB": "Theme colours",
          "zh-CN": "Theme-Farben"
        }
      },
      "typography": {
        "label": {
          "en-GB": "Typography",
          "zh-CN": "Typografie"
        }
      },
      "eCommerce": {
        "label": {
          "en-GB": "E-Commerce",
          "zh-CN": "E-Commerce"
        }
      },
      "statusColors": {
        "label": {
          "en-GB": "Status messages",
          "zh-CN": "Status-Ausgaben"
        }
      },
      "media": {
        "label": {
          "en-GB": "Media",
          "zh-CN": "Medien"
        }
      },
      "unordered": {
        "label": {
          "en-GB": "Misc",
          "zh-CN": "Sonstige"
        }
      }
    },
    "fields": {
      "sw-color-brand-primary": {
        "label": {
          "en-GB": "Primary colour",
          "zh-CN": "Primärfarbe"
        },
        "type": "color",
        "value": "#008490",
        "editable": true,
        "block": "themeColors",
        "order": 100
      },
      "sw-color-brand-secondary": {
        "label": {
          "en-GB": "Secondary colour",
          "zh-CN": "Sekundärfarbe"
        },
        "type": "color",
        "value": "#526e7f",
        "editable": true,
        "block": "themeColors",
        "order": 200
      },
      "sw-border-color": {
        "label": {
          "en-GB": "Border",
          "zh-CN": "Rahmen"
