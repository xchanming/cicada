{
  "name": "SwagTheme",
  "author": "cicada AG",
  "views": [
     "@Storefront",
     "@Plugins",
     "@SwagTheme"
  ],
  "style": [
    {
      "app/storefront/src/scss/overrides.scss": {}
    },
    "@Storefront",
    {
      "app/storefront/src/scss/base.scss": {
        "resolve": {
          "vendor": "app/storefront/vendor"
        }
      }
    }
  ],
  "script": [
    "@Storefront",
    "app/storefront/dist/js/main.js"
  ],
  "asset": [
    "app/storefront/dist/assets"
  ],
  "previewMedia": "app/storefront/dist/assets/preview.jpg",
  "config": {
    "fields": {
      "sw-image": {
        "type": "media",
        "value": "app/storefront/dist/assets/test.jpg",
        "label": {
          "en-GB": "test label",
          "zh-CN": "Test label"
        },
        "helpText": {
          "en-GB": "test help",
          "zh-CN": "Test Hilfe"
        }
      }
    }
  }
}
