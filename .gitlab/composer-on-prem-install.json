{
  "name": "cicada/production",
  "license": "MIT",
  "type": "project",
  "require": {
    "composer-runtime-api": "^2.0",
    "cicada/administration": "*",
    "cicada/core": "*",
    "cicada/elasticsearch": "*",
    "cicada/storefront": "*",
    "cicada/dev-tools": "*",
    "symfony/flex": "~2",
    "symfony/runtime": "^5.0|^6.0|^7.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "custom/plugins/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "custom/plugins/*/packages/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "custom/static-plugins/*",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "../src/*"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": [
    ],
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "endpoint": [
        "https://raw.githubusercontent.com/cicada/recipes/flex/main/index.json",
        "flex://defaults"
      ]
    }
  }
}
